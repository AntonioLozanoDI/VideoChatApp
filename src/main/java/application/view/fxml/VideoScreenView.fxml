<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>

<StackPane prefHeight="1000.0" prefWidth="1470.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.controller.VideoStreamingController">
   <children>
      <AnchorPane fx:id="videoPane" onMouseMoved="#mouseMoved">
         <children>
            <ImageView fx:id="imageView" onMouseMoved="#mouseMoved" pickOnBounds="true" preserveRatio="true" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
         </children>
      </AnchorPane>
      <AnchorPane fx:id="glassPane" onMouseMoved="#mouseMoved">
         <children>
            <Button fx:id="initCallButton" layoutX="637.0" layoutY="442.0" mnemonicParsing="false" onAction="#initCall" prefHeight="58.0" prefWidth="196.0" text="Iniciar videollamada">
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
            <Button fx:id="micButton" layoutX="688.0" layoutY="644.0" mnemonicParsing="false" onAction="#toggleMic" style="-fx-background-color: darkgrey;" styleClass="roundedButton" AnchorPane.leftAnchor="630.0" />
            <Button fx:id="speakerButton" layoutX="804.0" layoutY="645.0" mnemonicParsing="false" onAction="#toggleSpeaker" style="-fx-background-color: darkgrey;" styleClass="roundedButton" AnchorPane.rightAnchor="630.0" />
            <Button fx:id="pauseButton" layoutX="696.0" layoutY="644.0" mnemonicParsing="false" onAction="#pauseCall" style="-fx-background-color: darkgrey;" styleClass="roundedButton" AnchorPane.leftAnchor="696.0" />
            <Button fx:id="stopButton" layoutX="794.0" layoutY="644.0" mnemonicParsing="false" onAction="#stopCall" style="-fx-background-color: darkgrey;" styleClass="roundedButton" AnchorPane.rightAnchor="690.0" />
         </children>
      </AnchorPane>
   </children>
</StackPane>
